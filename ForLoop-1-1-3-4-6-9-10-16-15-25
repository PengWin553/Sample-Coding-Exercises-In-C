#include<stdio.h> // 1       1       3       4       6       9       10      16      15      25
int main()
{
    int i;
    int j = 1;
    int x = 1;

    for(i=1; i<=15 && j<=5; i+=0)
    {
        x++;
        printf("%d\t",i);
        printf("%d\t",j*j);
        
        j++;
        i = x+i;
    }

    return 0;
}